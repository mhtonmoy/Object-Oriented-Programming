#include<iostream>
using namespace std;
int main()
{
    int i,j;
    int a[3][3];
    cout<<"Enter array elements = ";
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
            cin>>a[i][j];
    }
    cout<<"Diagonal Matrix are = "<<endl;
    for(i=0;i<3;i++)
    {
       for(j=0;j<3;j++)
       {
           if(i==j)
            cout<<a[i][j]<<" ";
       }
    }
    return 0;
}
